; This file is for unifying the coding style for different editors and IDEs.
; More information at http://editorconfig.org
;
;

root = true

[*]
charset = utf-8
trim_trailing_whitespace = true
; insert_final_newline = false
;tab_width = 4

[*.js]
indent_size = 4
indent_style = space

[*.json]
indent_size = 2
indent_style = space

[*.{toml,snbt}]
indent_size = 4
indent_style = tab

[.git{attributes,config}]
indent_size = 4
indent_style = tab
